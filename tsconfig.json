{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "allowUnreachableCode":true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "declaration": true,
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*"]
}
